alias welcome="sudo /usr/local/bin/xentry -i"
# Toggle transparency lua settings in conky (changes dialog to use lua)
# Conky must be installed from the lilidog installers to use these functions.
alias conkylua="$HOME/.config/conky/scripts/conky-lua-toggle"
alias conkylua-awesome="$HOME/.config/awesome/conky-awesome/scripts/conky-lua-toggle-awesome"
